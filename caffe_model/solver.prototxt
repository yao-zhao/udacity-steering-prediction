train_net: "caffe_model/train.prototxt"
test_net: "caffe_model/val.prototxt"
type: "Nesterov"
test_iter: 32
# test_iter: 768
test_interval: 100
test_initialization: true
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 8000
display: 10
max_iter: 24000
momentum: 0.90
weight_decay: 1e-6
snapshot: 1000
snapshot_prefix: "caffe_model/resnet"
solver_mode: GPU
# iter_size: 1
