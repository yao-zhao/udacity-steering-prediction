layer {
  name: "fc1"
  type: "InnerProduct"
  bottom: "conv1"
  top: "fc1"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 1
  }
  inner_product_param {
    num_output: 35
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "discrete_label"
  type: "Discretize"
  bottom: "label"
  top: "discrete_label"
  top: "label_centers"
  discretize_param {
    separator: -6
    separator: -5
    separator: -4
    separator: -3
    separator: -2
    separator: -1
    separator: -0.7
    separator: -0.5
    separator: -0.4
    separator: -0.3
    separator: -0.2
    separator: -0.15
    separator: -0.1
    separator: -0.075
    separator: -0.05
    separator: -0.03
    separator: -0.01
    separator: 0.01
    separator: 0.03
    separator: 0.05
    separator: 0.075
    separator: 0.1
    separator: 0.15
    separator: 0.2
    separator: 0.3
    separator: 0.4
    separator: 0.5
    separator: 0.7
    separator: 1
    separator: 2
    separator: 3
    separator: 4
    separator: 5
    separator: 6
  }
}
layer {
  name: "softmax_decay_loss"
  type: "SoftmaxWithDecayLoss"
  bottom: "fc1"
  bottom: "discrete_label"
  top: "softmax_decay_loss"
  top: "softmax_decay_softmax"
  loss_weight: 1
  loss_weight: 0
  softmax_decay_loss_param {
    rate: 1.5
  }
}
layer {
  name: "softmax_decay_accuracy"
  type: "Accuracy"
  bottom: "fc1"
  bottom: "label"
  top: "softmax_decay_accuracy"
}
layer {
  name: "softmax_decay_interpolated_label"
  type: "SoftmaxInterpolation"
  bottom: "softmax_decay_softmax"
  bottom: "label_centers"
  top: "softmax_decay_interpolated_label"
}
layer {
  name: "softmax_decay_euclidean_loss"
  type: "EuclideanLoss"
  bottom: "softmax_decay_interpolated_label"
  bottom: "label"
  top: "softmax_decay_euclidean_loss"
  loss_weight: 0
}
